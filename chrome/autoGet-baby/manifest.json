{
  "name": "autoGet",
  "manifest_version": 2,
  "version": "1.0",
  "description": "baby",
  "browser_action": {
    "default_icon": "ok.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://yz.chsi.com.cn/yzwb/"
      ],
      "js": [
        "autoGet.js"
      ]
    },
    {
      "matches": [
        "http://static.mi.com/buySuccess/*"
      ],
      "js": [
        "goToCart.js"
      ]
    },
    {
      "matches": [
        "http://static.mi.com/cart/*"
      ],
      "js": [
        "checkOut.js"
      ]
    },
    {
      "matches": [
        "http://order.mi.com/*"
      ],
      "js": [
        "submitOrder.js"
      ]
    }
  ]
}